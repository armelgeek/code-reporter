{
  "name": "code-reporter",
  "displayName": "code-reporter",
  "description": "describe section of code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.activateContextMenu"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showCodePopup",
        "title": "Code Reporter",
        "description": "Generate a code report for the selected code",
        "category": "Code Report"
      }
    ]
  },
  "menus": {
    "editor/context": [
      {
        "command": "extension.showCodePopup",
        "group": "navigation"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "prod": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "vsce": "^2.15.0"
  }
}
